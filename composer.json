{
    "name": "tdt/input",
    "description": "",
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "tdt/json" : "dev-master",
        "semsol/arc2" : "dev-master"
    },
    "require-dev": {
        "mockery/mockery" : "dev-master",
        "phpunit/phpunit" : "4.0.*",
        "doctrine/dbal": "~2.3",
        "jenssegers/mongodb": "1.3.2"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "Tdt\\Input": "src/"
        }
    },
    "scripts": {
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-install-cmd": [
            "php artisan optimize",
            "php artisan asset:publish tdt/input"
        ],
        "post-update-cmd": [
            "php artisan optimize",
            "php artisan asset:publish tdt/input"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

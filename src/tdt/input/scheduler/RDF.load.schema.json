{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "title": "tdt/input job configuration schema for a RDF loader",
    "type": "object",
    "properties": {
        "endpoint" : {
            "type":"string",
            "required" : true
        },
        "datatank_uri" : {
            "type" : "string",
            "required" : true
        },
"datatank_package": {
            "type" : "string",
            "required" : true
        },
"datatank_resource": {
            "type" : "string",
            "required" : true
        },
"datatank_user": {
            "type" : "string",
            "required" : true
        },
"datatank_password": {
            "type" : "string",
            "required" : true
        },
        "endpoint_user": {
            "type" : "string",
            "required" : false
        },
"endpoint_password": {
            "type" : "string",
            "required" : false
        },
        "buffer_size": {
            "type" : "string",
            "required" : false
        }
    }
}